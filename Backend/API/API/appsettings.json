{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "ConnectionStrings": {
    "ConnString": "<connString>"
  },
  "RootPasswords": {
    "adminPassword": "<defaultAdminPassword>",
    "sysAdminPassword": "<defaultSysadminPassword>"
  },
  "Jwt": {
    "Token": "<JWTtoken>"
  },
  "MailDetails": {
    "Mail": "<email>",
    "Password": "<password>",
    "SmtpHost": "smtp.gmail.com",
    "Port": 587
  }
}
